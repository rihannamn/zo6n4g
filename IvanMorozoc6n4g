using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Drawing;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms;
using concolseT_6;
 
namespace fTask_6
{
   public partial class Zad1 : Form
   {
       public Zad1()
       {
           InitializeComponent();
       }
 
       private void button1_Click(object sender, EventArgs e)
       {
           try
           {
               Task1 t = new Task1(int.Parse(tbN.Text));
               labArr.Text = "Одномерный массив\n" + t.ShowArr();
               lab1Proizv.Text = "Произедение элементов:\n" + t.ArrMultiple;
               labIsThree1.Text = "Произведение трехзначное?\n" + (t.ThreeZnachForArr?"Да":"Нет");
           }
           catch(System.FormatException)
           {
               MessageBox.Show("Ввели что-то не то", "Ошибка");
           }
           
       }
 
       private void button2_Click(object sender, EventArgs e)
       {
           try
           {
               Task1 t = new Task1(int.Parse(tbN2.Text), int.Parse(tbM.Text));
               labArr2.Text = "Двумерный массив\n" + t.Show2mArr();
               labProizv2.Text = "Произедение:\n" + t.Arr2Multiple;
               labIsThree2.Text = "Произведение трехзначное?\n" + (t.ThreeZnachForArr2? "Да" : "Нет");
           }
           catch (System.FormatException)
           {
               MessageBox.Show("Ввели что-то не то", "Ошибка");
           }
       }
   }
}
 
 
